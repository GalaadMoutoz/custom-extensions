{
  "name": "vuetify-extensions",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "nuxt generate && firebase deploy",
    "deploy:hosting": "nuxt generate && firebase deploy --only hosting",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "emulators": "firebase emulators:start --import=./firebase-emulators-data --export-on-exit",
    "functions": "cd functions && tsc -w"
  },
  "dependencies": {
    "@galaadmoutoz/vuex-firestore": "^1.0.1",
    "@nuxtjs/firebase": "^7.6.1",
    "@types/grecaptcha": "^3.0.3",
    "core-js": "^3.15.1",
    "firebase": "^8.9.0",
    "nuxt": "^2.15.7",
    "nuxt-property-decorator": "^2.9.1",
    "vuedraggable": "^2.24.3",
    "vuetify": "^2.5.5",
    "xlsx": "^0.17.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.15",
    "@babel/preset-env": "7.13.15",
    "@babel/eslint-parser": "^7.14.7",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/vuetify": "^1.12.1",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "prettier": "^2.3.2"
  }
}
